{
    "name": "diller-loyalty/php-sdk",
    "version": "v0.1.27",
    "description": "PHP SDK for Diller API",
    "keywords": [
        "diller",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://diller.io",
    "license": "proprietary",
    "authors": [
        {
            "name": "Diller",
            "email": "dillertechsupport@diller.no",
            "homepage": "https://diller.io"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": ">=7.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.3",
        "fakerphp/faker": "v1.21.0",
        "squizlabs/php_codesniffer": "~2.6",
        "vlucas/phpdotenv": "v5.5.0"
    },
    "autoload": {
        "psr-4": {
            "DillerAPI\\" : "lib/"
